IN	START
	LD	GR1,IN1		; GR1に1つめの引数を
	LD	GR2,IN2		; GR2に2つめの引数を
	CALL	ADDER		; サブルーチンADDERを呼び出す
	CPA	GR3,=5		; GR3が正しく5になっているかチェック
	JNZ	NG
	OUT	OKMSG,OKLEN
	JUMP	FIN
NG	OUT	NGMSG,NGLEN
FIN	RET
IN1	DC	3
IN2	DC	2
NGMSG	DC	'WRONG!'
NGLEN	DC	6
OKMSG	DC	'CORRECT!'
OKLEN	DC	8
	END

ADDER	START
	ADDA	GR1,GR2		; ここの処理を埋める
	LD	GR3,GR1
	RET
	END
